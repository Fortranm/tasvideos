@model PublicationPoints
<standard-table>
	<tr>
		<th>Pos</th>
		<th>Movie</th>
		<th>Points</th>
	</tr>
	@foreach (var item in Model.Pubs)
	{
		<tr>
			<td>@item.Position</td>
			<td>
				<pub-link id="@item.Id">@item.Title</pub-link>
			</td>
			<td>@Math.Round(item.Points, 3)</td>
		</tr>
	}
</standard-table>
