<form asp-page="/Users/Profile" method="GET" id="search-form">
	<div class="input-group">
		<label class="input-group-text" for="search-username-box">Search: </label>
		<input id="search-username-box" data-user-search="true" name="name" list="search-username-list" required="required" type="search" class="form-control" placeholder="Enter a UserName" autofocus="autofocus" autocomplete="off" spellcheck="false" />
		<submit-button id="go-button" btn-class-override="btn-secondary" disabled="diabled">Go</submit-button>
	</div>
	<script src="/js/user-search.js"></script>
</form>