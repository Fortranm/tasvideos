@page
@model CacheSetModel
@{
	ViewData.SetTitle("Set a cache value (only works on strings!)");
}
<ul>
	<li><a asp-page="Index">Diagnostics</a></li>
	<li><a asp-page="CacheControl" class="mt-2">Cache Control</a></li>
</ul>
<row>
	<column md="6">
		<form method="post">
			<fieldset>
				<label asp-for="Key"></label>
				<input asp-for="Key" type="text" placeholder="key" class="form-control" required />
			</fieldset>
			<fieldset>
				<label asp-for="Value"></label>
				<input asp-for="Value" type="text" placeholder="value" class="form-control" required />
			</fieldset>
			<form-button-bar>
				<button type="submit" class="btn btn-primary">Set</button>
			</form-button-bar>
		</form>
	</column>
</row>
